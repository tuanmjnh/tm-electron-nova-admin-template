{
  "name": "tm-electron-nova-admin-template",
  "private": true,
  "version": "1.0.0",
  "description": "TM Electron Nova Admin Template",
  "author": "tuanmjnh",
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev --port 9980",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "build": "vue-tsc && vite build --mode prod",
    "build:dev": "vue-tsc && vite build --mode dev",
    "build:test": "vue-tsc && vite build --mode test",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "preview": "vite preview",
    "lint": "eslint . && vue-tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "lint:check": "npx @eslint/config-inspector",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@vueuse/core": "^13.3.0",
    "alova": "^3.2.13",
    "colord": "^2.9.3",
    "echarts": "^5.6.0",
    "md-editor-v3": "^5.6.1",
    "moment": "^2.30.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.3.0",
    "quill": "^2.0.3",
    "radash": "^12.1.0",
    "vue": "^3.5.16",
    "vue-draggable-plus": "^0.6.0",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.3",
    "@iconify-json/icon-park-outline": "^1.2.2",
    "@iconify/vue": "^5.0.0",
    "@types/node": "^22.15.30",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^9.28.0",
    "lint-staged": "^16.1.0",
    "naive-ui": "^2.41.0",
    "sass": "^1.89.1",
    "typescript": "^5.8.3",
    "unocss": "^66.1.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vue-tsc": "^2.2.10"
  },
  "main": "dist-electron/main.js"
}